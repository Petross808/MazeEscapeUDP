VisualElement {
}

.button {
    width: 16%;
    height: 26%;
    position: relative;
    color: rgba(27, 27, 27, 0);
    -unity-text-outline-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgba(255, 255, 255, 0);
    background-color: rgba(0, 0, 0, 0);
}

.header {
    width: 50%;
    height: 30%;
    font-size: 80px;
    color: rgb(255, 255, 255);
}

.button:hover {
    -unity-background-image-tint-color: rgb(255, 148, 0);
    scale: 1.1 1.1;
}
